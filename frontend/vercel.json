{
  "rewrites": [
    { "source": "/api/projects/:path*", "destination": "/api/proxy" },
    { "source": "/project/:path*", "destination": "/" },
    { "source": "/settings", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
